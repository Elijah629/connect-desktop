{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ESNext",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"sourceMap": false,
		"baseUrl": ".",
		"outDir": "dist",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"removeComments": true,
		"paths": {
			"*": ["node_modules/*"]
		},
		"jsx": "react"
	},
	"include": ["src/**/*"]
}
